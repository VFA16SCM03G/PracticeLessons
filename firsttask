public class sample {

    public static void main(String[] args) {

        int marks[] = new int[5];
        int z;
        float sum=0;
        float average;
        Scanner sc = new Scanner(System.in);

        System.out.print("Enter the Marks Obtained in 5 Subjects : ");
        for(z=0; z<5; z++) {
            marks[z] = sc.nextInt();
            sum = sum + marks[z];
        }
//
        average = sum/5;

        System.out.print("Average Marks of Five Subjects are = " +average);
    }
}
